<?xml version="1.0" encoding="utf-8"?>
<transition xmlns:android="http://schemas.android.com/apk/res/android" android:exitFadeDuration="@android:integer/config_shortAnimTime">
    <item android:state_selected="true">
        <shape>
            <corners android:radius="14dp"/>
            <solid android:color="#e0e0e0"/>
        </shape>
    </item>
    <item android:state_selected="false">
        <shape>
            <corners android:radius="14dp"/>
            <solid android:color="@color/wy"/>
        </shape>
    </item>
</transition>
