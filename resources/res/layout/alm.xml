<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns="" background="@color/l" layout_width="match_parent" layout_height="wrap_content">
    <RelativeLayout orientation="vertical" id="@+id/cvf" padding="10dp" layout_width="match_parent" layout_height="64dp">
        <ImageView id="@+id/ble" layout_width="60dp" layout_height="50dp" scaleType="centerCrop" layout_alignParentLeft="true" layout_alignParentTop="true"/>
        <TextView textSize="15dp" textColor="@color/m0" ellipsize="end" id="@+id/title" layout_width="wrap_content" layout_height="wrap_content" layout_marginLeft="10dp" maxLines="1" includeFontPadding="false" layout_toRightOf="@+id/ble" layout_alignTop="@+id/ble"/>
        <TextView textSize="14dp" textColor="@color/lz" ellipsize="end" id="@+id/ajd" layout_width="wrap_content" layout_height="wrap_content" layout_marginTop="4dp" maxLines="1" includeFontPadding="false" layout_below="@+id/title" layout_alignLeft="@+id/title"/>
    </RelativeLayout>
</FrameLayout>
