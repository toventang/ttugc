<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3dp"/>
            <gradient android:startColor="@color/jg" android:endColor="@color/c9" android:angle="0" android:type="radial" android:gradientRadius="400" android:centerColor="@color/jg"/>
        </shape>
    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/l"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>
</layer-list>
